      "source": [
        "import numpy as np\n",
        "\n",
        "# 行列 A と B の定義\n",
        "A = np.array([[0, 1], [2, 3], [4, 5]])\n",
        "B = np.array([[0, 1, 2, 3], [4, 5, 6, 7]])\n",
        "\n",
        "# 行列の積を計算\n",
        "C = np.dot(A, B)\n",
        "\n",
        "# 結果を表示\n",
        "print(\"行列の積 C:\")\n",
        "print(C)\n",
        "\n",
        "# 行列 C の要素の最大値を計算\n",
        "max_value = np.max(C)\n",
        "print(\"行列 C の要素の最大値:\", max_value)"
      ]
